public class PageManager()
{

	public static List<Page> pages = new ArrayList<>();

	public static currentPage;

	public PageManager() {
		
			
	}

	public void update() {
		for (Page p : pages) {
			p.update();
		}
	}

	public void draw(SpriteBatch sb) {
		for (Page p : pages) {
			p.draw(sb);
		}
	}
}